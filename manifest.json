{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "tabs", "scripting"],
  "content_scripts": [
    {
      "js": ["./scripts/host.js"],
      "matches": ["https://www.youtube.com/watch?v=*"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "action": {
    "default_icon": "images/logo.png"
  }
}
